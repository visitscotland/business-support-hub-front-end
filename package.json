{
  "name": "nuxt-bloomreach-headless-white-label",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "postinstall": "node -p \"require('./node_modules/@visitscotland/component-library/package.json').version\" > .clversion && nuxt prepare",
    "commit": "cz",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "lint": "eslint ./**/*.{js,vue,ts}",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.3",
    "@nuxt/scripts": "^0.11.10",
    "@nuxtjs/dotenv": "^1.4.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.10",
    "nuxt": "~3.19.0",
    "nuxt-lazy-hydrate": "^1.0.0"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "^23.3.0",
    "@bloomreach/vue3-sdk": "^22.0.5",
    "@pinia/nuxt": "^0.5.1",
    "@visitscotland/component-library": "^5.9.0",
    "aws-rum-web": "^1.25.0",
    "axios": "^1.13.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "mitt": "^3.0.0",
    "nuxt-jsonld": "^2.0.8",
    "pinia": "^2.2.0",
    "sass": "^1.77.4",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.2",
    "unenv": "^1.10.0",
    "youtube-vue3": "^0.1.15"
  },
  "overrides": {
    "minimist": "latest",
    "minimatch": "9.0.3"
  },
  "resolutions": {
    "minimist": "latest",
    "minimatch": "9.0.3",
    "vite": "6.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
